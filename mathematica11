    function f1(n, byte, c) { // inverse of f2: f1(f2(x, y, c), c, y, c) == x
      for (var bitIndex = 0; bitIndex <= 7; bitIndex++) {
        var bit = (byte >> bitIndex) & 1;
        if (bit + ((n - bit) & ~1) == n) {
          n = (n - bit) >> 1;
        } else {
          n = ((c - bit) ^ n) >> 1;
        }
      }
      return n;
    }
    
    /*function f2(n, byte, c) {
      for (var bitIndex = 7; bitIndex >= 0; bitIndex--) {
